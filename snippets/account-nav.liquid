{{ 'main-order-main-account.css' | asset_url | stylesheet_tag }}

<nav class="main-order-main-account__nav">
  <ul>
    <li>
      <a href="{{ routes.account_url }}" class="{% if current == "account" %}txt-link{% endif %}">
        {{- 'customer.account.title' | t -}}
      </a>
    </li>

    <li >
      <a href="{{ routes.account_addresses_url }}" class="{% if current == "addresses" %}txt-link{% endif %}">
        {{- 'customer.addresses.title' | t -}}
      </a>
    </li>
    <li>
      <a href="{{ routes.account_logout_url }}">
        {{- 'customer.log_out' | t -}}
      </a>
    </li>
  </ul>
</nav>