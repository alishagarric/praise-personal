{
  "sections": {
    "main": {
      "type": "main-article",
      "blocks": {
        "featured_image": {
          "type": "featured_image",
          "settings": {
            "crop_shape": "rounded-wide",
            "overlay": 10
          }
        },
        "title": {
          "type": "title",
          "settings": {
            "heading_size": "h1",
            "overlap": true
          }
        },
        "details": {
          "type": "details",
          "settings": {
            "blog_show_date": true,
            "blog_show_author": false
          }
        },
        "content": {
          "type": "content",
          "settings": {
            "image_corners": "rounded"
          }
        },
        "sidebar": {
          "type": "sidebar",
          "settings": {
            "label": "Ingredients"
          }
        }
      },
      "block_order": [
        "featured_image",
        "title",
        "details",
        "content",
        "sidebar"
      ]
    }
  },
  "order": ["main"]
}
